{% extends 'base.html.twig' %}

{% block title %}Version{% endblock %}
{% block pageTitle %} Archive de la société {{version.name }} {% endblock %}

{% block body %}

    <table class="table">
        <tbody>
            <tr>
                <th>Name</th>
                <td>{{ version.name }}</td>
            </tr>
            <tr>
                <th>SirenNumber</th>
                <td>{{ version.sirenNumber }}</td>
            </tr>
            <tr>
                <th>CityOfRegistration</th>
                <td>{{ version.cityOfRegistration }}</td>
            </tr>
            <tr>
                <th>Registration</th>
                <td>{{ version.registration ? version.registration|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Capital</th>
                <td>{{ version.capital }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ version.updatedAt ? version.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>forme juridique</th>
                <td>{{ version.legalForm ? version.legalForm.name : '' }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>
                    {% for address in version.addresses %}
                        {{ address.number ~ ' ' ~ address.channelType ~ ' ' ~ address.name ~ ' ' ~ address.postalCode ~ ', ' }}
                    {% endfor %}
                </td>
            </tr>
        </tbody>
    </table>
{% endblock %}
