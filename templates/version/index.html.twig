{% extends 'base.html.twig' %}

{% block title %}Version index{% endblock %}

{% block body %}
    <h1>Version index</h1>

    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>SirenNumber</th>
            <th>CityOfRegistration</th>
            <th>Registration</th>
            <th>Capital</th>
            <th>UpdatedAt</th>
            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for version in versions %}
            <tr>
                <td>{{ version.id }}</td>
                <td>{{ version.name }}</td>
                <td>{{ version.sirenNumber }}</td>
                <td>{{ version.cityOfRegistration }}</td>
                <td>{{ version.registration ? version.registration|date('Y-m-d') : '' }}</td>
                <td>{{ version.capital }}</td>
                <td>{{ version.updatedAt ? version.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('version_show', {'id': version.id}) }}">show</a>
                    <a href="{{ path('version_edit', {'id': version.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('version_new') }}">Create new</a>
{% endblock %}
